    <script>
        $("#myProperties").click(function () {
            $('#ownerShowprofile').empty();
            $('#ownerShowproperty').load('/owner/getpropertylistPartial');
            return false;
        });
        $("#showprofile").click(function () {
            $('#ownerShowproperty').empty();
            $('#ownerShowprofile').load('/common/ViewprofilePartial');
            return false;
        });
        $("#tenantprofile").click(function () {
            $('#tenantShowPayment').empty();
            $('#tenantShowServiceRequest').empty();
            $('#tenantShowprofile').load('/common/ViewprofilePartial');
            return false;
        });
        $("#tenantPayment").click(function () {
            $('#tenantShowprofile').empty();
            $('#tenantShowServiceRequest').empty();
            $('#tenantShowPayment').load('/Tenant/myPaymentsPartial');
            return false;
        });
        $("#tenantServiceRequest").click(function () {
            $('#tenantShowprofile').empty();
            $('#tenantShowPayment').empty();
            $('#tenantShowServiceRequest').load('/Tenant/serviceRequestPartial');
            return false;
        });
        $(window).load(function () {
            $('#tenantShowPayment').load('/Tenant/myPaymentsPartial');
            return false;
        });

        $("#alreadypaid").click(function () {
            $('#paynow').show();

        });
        $("#unpaidpaid").click(function () {
            $('#paynow').hide();

        });




   </script>

