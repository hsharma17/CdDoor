            $("#getpropertylist").click(function () {
                $('#loadhowitworks').empty();
                $('#loadpropertiesnearby').empty();
                $('#loadPropertyList').load('/home/show_propertiesPartial');
                $('#loadlatestreviews').load('/home/latest_reviewsPartial');
                $('#loadrecentproperties').load('/home/recent_propertiesPartial');
                $('#loadadvancesearch').load('/home/advance_searchPartial');
                return false;
            });
            $(window).load(function () {
                $('#loadpropertiesnearby').load('/home/propertiesnearby');
                $('#loadhowitworks').load('/home/howitworksPartial');
                
                return false;
            });
            $("#getaboutus").click(function () {
                $('#loadaboutus').load('/home/aboutusPartial');
                return false;
            });
            $("#loadgetstarted1,#loadgetstarted2,#loadgetstarted3").click(function () {
                $('html, body').animate({
                    scrollTop: $("#loadsearcharea").offset().top
                }, 1000);
            });
            
            $("#loadlearnmore1,#loadlearnmore2,#loadlearnmore3").click(function () {
                $('html, body').animate({
                    scrollTop: $("#loadpropertiesnearby").offset().top
                }, 1000);
