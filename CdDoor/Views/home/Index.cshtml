@{
    ViewBag.title = "ClickDoor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Banner start -->
<div class="banner">
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src=@Url.Content("~/img/banner/banner-slider-1.jpg") alt="banner-slider-1">
                <div class="carousel-caption banner-slider-inner banner-top-align">
                    <div class="text-center">
                        <h1 data-animation="animated fadeInDown delay-05s"><span>Welcome to </span>ClickDoor</h1>
                        <p class="hidden-xs"></p>
                        <a href="#" class="btn button-md button-theme" id="loadgetstarted1" data-animation="animated fadeInUp delay-05s">Get Started Now</a>
                        <a href="#" class="btn button-md border-button-theme" id="loadlearnmore1" data-animation="animated fadeInUp delay-05s">Learn More</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src=@Url.Content("~/img/banner/banner-slider-2.1.jpg") alt="banner-slider-2">
                <div class="carousel-caption banner-slider-inner banner-top-align">
                    <div class="text-right">
                        <h1 data-animation="animated fadeInDown delay-05s"><span>Sweet Home For</span> <br>Small Family</h1>
                        <a href="#" class="btn button-md button-theme" id="loadgetstarted2" data-animation="animated fadeInUp delay-05s">Get Started Now</a>
                        <a href="#" class="btn button-md border-button-theme" id="loadlearnmore2" data-animation="animated fadeInUp delay-05s">Learn More</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src=@Url.Content("~/img/banner/banner-slider-3.jpg") alt="banner-slider-3">
                <div class="carousel-caption banner-slider-inner banner-top-align">
                    <div class="text-left">
                        <h1 data-animation="animated fadeInDown delay-05s"><span>Best Place To live</span><br> for Bachelors</h1>
                        <a href="#" class="btn button-md button-theme" id="loadgetstarted3" data-animation="animated fadeInUp delay-05s">Get Started Now</a>
                        <a href="#" class="btn button-md border-button-theme" id="loadlearnmore3" data-animation="animated fadeInUp delay-05s">Learn More</a>
                    </div>
                </div>
            </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
            <span class="slider-mover-left" aria-hidden="true">
                <i class="fa fa-angle-left"></i>
            </span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
            <span class="slider-mover-right" aria-hidden="true">
                <i class="fa fa-angle-right"></i>
            </span>
            <span class="sr-only">Next</span>
        </a>
    </div>


<!-- Banner end -->
<!-- Search area start -->
<div class="search-area" id="loadsearcharea">
    <div class="container">
        <div class="search-area-inner">
            <div class="search-contents ">
                <form action="" method="GET">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6">
                            <div class="form-group">
                                <input style="padding-left:15px;" id="locationTextField" type="text" placeholder="Search landmarks or pincodes" size="35" class="selectpicker search-fields">
                            </div>
                        </div>
                        <div class="col-lg-2 col-md-3 col-sm-6 col-xs-6">
                            <div class="form-group">
                                <select class="selectpicker search-fields" name="tenant-type">
                                    <option>Students</option>
                                    <option>Professionals</option>
                                   
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-2 col-md-3 col-sm-6 col-xs-6">
                            <div class="form-group">
                                <select class="selectpicker search-fields" name="property-For">
                                    <option>Who</option>
                                    <option>Boys</option>
                                    <option>Girls</option>
                                    <option>Family</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-2 col-md-3 col-sm-6 col-xs-6">
                            <div class="form-group">
                                <select class="selectpicker search-fields" name="house-type">
                                    <option>House Type</option>
                                    <option>Apartments</option>
                                    <option>Flats</option>
                                    <option>Villas</option>
                                    <option>Pgs</option>                                   
                                </select>
                            </div>
                        </div>
                        
                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 ">
                            <div class="form-group">
                                <button class="search-button" id="getpropertylist">Search</button>
                            </div>
                    </div>
                   
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
